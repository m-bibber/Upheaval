	NAME:AlliedGunshipAircraft
		"Harbinger-Gunship"
	End
	
	DESC:AlliedGunshipAircraft
		"Einige führende alliierte Politiker haben vehement gegen die Anschaffung dieses riesigen Flugzeuges protestiert."
	End
	
	TYPE:AlliedGunshipAircraft
		"Verbesserter Unterstützungsbomber"
	End
	
	NAME:ToggleAlliedGunship
		"Schnellfeuergeschütz"
	End
	
